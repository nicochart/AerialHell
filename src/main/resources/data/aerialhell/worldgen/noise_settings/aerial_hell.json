{
  "sea_level": -64,
  "disable_mob_generation": false,
  "aquifers_enabled": false,
  "ore_veins_enabled": false,
  "legacy_random_source": true,
  "default_block": {
    "Name": "aerialhell:stellar_stone"
  },
  "default_fluid": {
    "Name": "minecraft:water",
    "Properties": {
      "level": "0"
    }
  },
  "noise": {
    "min_y": 0,
    "height": 256,
    "size_horizontal": 2,
    "size_vertical": 1,
    "sampling": {
      "xz_scale": 2.6,
      "y_scale": 1.3,
      "xz_factor": 80,
      "y_factor": 160
    },
    "bottom_slide": {
      "target": -1,
      "size": 20,
      "offset": -10
    },
    "top_slide": {
      "target": -2,
      "size": 70,
      "offset": -43
    },
    "terrain_shaper": {
      "offset": 0,
      "factor": 0,
      "jaggedness": 0
    }
  },
  "noise_router": {
    "barrier": 0,
    "fluid_level_floodedness": 0,
    "fluid_level_spread": 0,
    "lava": 0,
    "temperature": {
      "type": "minecraft:shifted_noise",
      "noise": "minecraft:temperature",
      "xz_scale": 0.25,
      "y_scale": 0,
      "shift_x": "minecraft:shift_x",
      "shift_y": 0,
      "shift_z": "minecraft:shift_z"
    },
    "vegetation": {
      "type": "minecraft:shifted_noise",
      "noise": "minecraft:vegetation",
      "xz_scale": 0.25,
      "y_scale": 0,
      "shift_x": "minecraft:shift_x",
      "shift_y": 0,
      "shift_z": "minecraft:shift_z"
    },
    "continents": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/continents"
    },
    "erosion": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/erosion"
    },
    "depth": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/depth"
    },
    "ridges": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/ridges"
    },
    "initial_density_without_jaggedness": {
      "type": "minecraft:mul",
      "argument1": 4,
      "argument2": {
        "type": "minecraft:quarter_negative",
        "argument": {
          "type": "minecraft:mul",
          "argument1": "minecraft:overworld/depth",
          "argument2": {
            "type": "minecraft:cache_2d",
            "argument": "minecraft:overworld/factor"
          }
        }
      }
    },
    "final_density": {
      "type": "minecraft:add",
      "argument1": {
        "type": "minecraft:squeeze",
        "argument": {
          "type": "minecraft:mul",
          "argument1": {
            "type": "minecraft:blend_density",
            "argument": {
              "type": "minecraft:add",
              "argument1": {
                "type": "minecraft:interpolated",
                "argument": 0
                //"argument": {
                //  "type": "minecraft:slide", //slide no longer exists
                //  "argument": "minecraft:overworld/sloped_cheese"
                //}
              },
              "argument2": {
                "type": "minecraft:mul",
                "argument1": {
                  "type": "minecraft:range_choice",
                  "input": {
                    "type": "minecraft:blend_density",
                    "argument": {
                      "type": "minecraft:mul",
                      "argument1": { //noodle detection
                        "type": "minecraft:range_choice",
                        "input": "minecraft:overworld/caves/noodle",
                        "min_inclusive": 64, //noodle caves values are 0 or 64
                        "max_exclusive": 65,
                        "when_in_range": 1,
                        "when_out_of_range": 0
                      },
                      "argument2": { //y>64 detection
                        "type": "minecraft:range_choice",
                        "input": {
                          "type": "minecraft:y_clamped_gradient",
                          "from_y": 0,
                          "to_y": 320,
                          "from_value": 0,
                          "to_value": 320
                        },
                        "min_inclusive": 65,
                        "max_exclusive": 400,
                        "when_in_range": 1,
                        "when_out_of_range": 0
                      }
                    }
                  },
                  "min_inclusive": 1,
                  "max_exclusive": 2,
                  "when_in_range": { //affects terrain "inside the noodles" & y>64 : higher value = less land
                    "type": "minecraft:y_clamped_gradient",
                    "from_y": 0,
                    "to_y": 320,
                    "from_value": 130, //less land at low altitude
                    "to_value": -120 //more land at high altitude
                  },
                  "when_out_of_range": { //affects terrain at y<65 & "ouside the noodles" : higher value = less land
                    "type": "minecraft:mul",
                    "argument1": {
                      "type": "minecraft:y_clamped_gradient",
                      "from_y": 0,
                      "to_y": 320,
                      "from_value": -42, //more land at low altitude
                      "to_value": 230 //less land at high altitude
                    },
                    "argument2": { //blocks suppression at low heigh level
                      "type": "minecraft:y_clamped_gradient",
                      "from_y": 0,
                      "to_y": 20,
                      "from_value": 0,
                      "to_value": 1
                    }
                  }
                },
                "argument2": -0.009
              }
            }
          },
          "argument2": {
            "type": "minecraft:mul",
            "argument1": { //smoothing the y=64 layer when in noodle
              "type": "minecraft:range_choice",
              "input": "minecraft:overworld/caves/noodle",
              "min_inclusive": 64, //noodle selection
              "max_exclusive": 65,
              "when_in_range": {
                "type": "minecraft:range_choice",
                "input": {
                  "type": "minecraft:y_clamped_gradient",
                  "from_y": 0,
                  "to_y": 256,
                  "from_value": 0,
                  "to_value": 256
                },
                "min_inclusive": 55, //y selection
                "max_exclusive": 71,
                "when_in_range": {
                  "type": "minecraft:mul",
                  "argument1": { //from 55 to 64 smoothing
                    "type": "minecraft:range_choice",
                    "input": {
                      "type": "minecraft:y_clamped_gradient",
                      "from_y": 55,
                      "to_y": 71,
                      "from_value": 55,
                      "to_value": 71
                    },
                    "min_inclusive": 55,
                    "max_exclusive": 64,
                    "when_in_range": {
                      "type": "minecraft:y_clamped_gradient",
                      "from_y": 55,
                      "to_y": 64,
                      "from_value": 1,
                      "to_value": 0
                    },
                    "when_out_of_range": 1
                  },
                  "argument2": { //from 64 to 71 smoothing
                    "type": "minecraft:range_choice",
                    "input": {
                      "type": "minecraft:y_clamped_gradient",
                      "from_y": 55,
                      "to_y": 71,
                      "from_value": 55,
                      "to_value": 71
                    },
                    "min_inclusive": 64,
                    "max_exclusive": 71,
                    "when_in_range": {
                      "type": "minecraft:y_clamped_gradient",
                      "from_y": 64,
                      "to_y": 71,
                      "from_value": 0,
                      "to_value": 1
                    },
                    "when_out_of_range": 1
                  }
                },
                "when_out_of_range": 1
              },
              "when_out_of_range": 1
            },
            "argument2": {
              "type": "minecraft:range_choice",
              "input": {
                "type": "minecraft:y_clamped_gradient",
                "from_y": 0,
                "to_y": 256,
                "from_value": 0,
                "to_value": 256
              },
              "min_inclusive": 240,
              "max_exclusive": 257,
              "when_in_range": {
                "type": "minecraft:y_clamped_gradient",
                "from_y": 240,
                "to_y": 258,
                "from_value": 1,
                "to_value": 0
              },
              "when_out_of_range": 1
            }
          }
        }
      },
      "argument2": {
        "type": "minecraft:add",
        "argument1": {
          "type": "minecraft:interpolated",
          "argument": {
            "type": "minecraft:mul",
            "argument1": {
              "type": "minecraft:add",
              "argument1": "minecraft:overworld/caves/spaghetti_roughness_function",
              "argument2": "minecraft:overworld/caves/spaghetti_2d"
            },
            "argument2": -0.065
          }
        },
        "argument2": { //caves
          "type": "minecraft:range_choice",
          "input": {
            "type": "minecraft:noise",
            "noise": "minecraft:cave_entrance",
            "xz_scale": 1.2,
            "y_scale": 1.2
          },
          "min_inclusive": 0.2,
          "max_exclusive": 0.38,
          "when_in_range": -1,
          "when_out_of_range": 0
        }
      }
    },
    "vein_toggle": 0,
    "vein_ridged": 0,
    "vein_gap": 0
  },
  "surface_rule": {
    "type": "minecraft:sequence",
    "sequence": [
      {
        "type": "minecraft:condition",
