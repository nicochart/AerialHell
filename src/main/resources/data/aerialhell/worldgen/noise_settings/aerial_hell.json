{
  "sea_level": -64,
  "disable_mob_generation": false,
  "aquifers_enabled": false,
  "ore_veins_enabled": false,
  "legacy_random_source": true,
  "default_block": {
    "Name": "aerialhell:stellar_stone"
  },
  "default_fluid": {
    "Name": "minecraft:water",
    "Properties": {
      "level": "0"
    }
  },
  "noise": {
    "min_y": 0,
    "height": 256,
    "size_horizontal": 2,
    "size_vertical": 1,
    "sampling": {
      "xz_scale": 2.6,
      "y_scale": 1.3,
      "xz_factor": 80,
      "y_factor": 160
    },
    "bottom_slide": {
      "target": -1,
      "size": 20,
      "offset": -10
    },
    "top_slide": {
      "target": -2,
      "size": 70,
      "offset": -43
    },
    "terrain_shaper": {
      "offset": 0,
      "factor": 0,
      "jaggedness": 0
    }
  },
  "noise_router": {
    "barrier": 0,
    "fluid_level_floodedness": 0,
    "fluid_level_spread": 0,
    "lava": 0,
    "temperature": {
      "type": "minecraft:shifted_noise",
      "noise": "minecraft:temperature",
      "xz_scale": 0.25,
      "y_scale": 0,
      "shift_x": "minecraft:shift_x",
      "shift_y": 0,
      "shift_z": "minecraft:shift_z"
    },
    "vegetation": {
      "type": "minecraft:shifted_noise",
      "noise": "minecraft:vegetation",
      "xz_scale": 0.25,
      "y_scale": 0,
      "shift_x": "minecraft:shift_x",
      "shift_y": 0,
      "shift_z": "minecraft:shift_z"
    },
    "continents": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/continents"
    },
    "erosion": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/erosion"
    },
    "depth": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/depth"
    },
    "ridges": {
      "type": "minecraft:mul",
      "argument1": 1,
      "argument2": "minecraft:overworld/ridges"
    },
    "initial_density_without_jaggedness": {
      "type": "minecraft:mul",
      "argument1": 4,
      "argument2": {
        "type": "minecraft:quarter_negative",
        "argument": {
          "type": "minecraft:mul",
          "argument1": "minecraft:overworld/depth",
          "argument2": {
            "type": "minecraft:cache_2d",
            "argument": "minecraft:overworld/factor"
          }
        }
      }
    },
    "final_density": {
      "type": "minecraft:add",
      "argument1": {
        "type": "minecraft:squeeze",
        "argument": {
          "type": "minecraft:mul",
          "argument1": 0.9,
          "argument2": {
            "type": "minecraft:interpolated",
            "argument": {
              "type": "minecraft:blend_density",
              "argument": {
                "type": "minecraft:add",
                "argument1": {
                  "type": "minecraft:slide",
                  "argument": "minecraft:end/sloped_cheese"
                },
                "argument2": {
                  "type": "minecraft:mul",
                  "argument1": {
                    "type": "minecraft:range_choice",
                    "input": {
                      "type": "minecraft:interpolated",
                      "argument": "minecraft:overworld/caves/noodle"
                    },
                    "min_inclusive": 64, //noodle caves values are 0 or 64
                    "max_exclusive": 65,
                    "when_in_range": {
                      "type": "minecraft:range_choice",
                      "input": "minecraft:overworld/continents",
                      "min_inclusive": -100,
                      "max_exclusive": 0,
                      "when_in_range": -56, //affects moutains : higher negatives values = larger moutains ; positive values = no moutains
                      "when_out_of_range": 40 //affects terrain "inside the noodles" : higher value = less land
                    },
                    "when_out_of_range": 10 //affects terrain "outside the noodles" : higher value = less land
                  },
                  "argument2": -0.008
                }
              }
            }
          }
        }
      },
      "argument2": {
        "type": "minecraft:interpolated",
        "argument": {
          "type": "minecraft:mul",
          "argument1": {
            "type": "minecraft:add",
            "argument1": "minecraft:overworld/caves/spaghetti_roughness_function",
            "argument2": "minecraft:overworld/caves/spaghetti_2d"
          },
          "argument2": -0.065
        }
      }
    },
    "vein_toggle": 0,
    "vein_ridged": 0,
    "vein_gap": 0
  },
  "surface_rule": {
    "type": "minecraft:sequence",
    "sequence": [
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:stone_depth",
          "offset": 0,
          "surface_type": "floor",
          "add_surface_depth": false,
          "secondary_depth_range": 0
        },
        "then_run": {
          "type": "minecraft:sequence",
          "sequence": [
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:biome",
                "biome_is": [
                  "aerialhell:giant_red_mushroom_forest",
                  "aerialhell:verdigris_agaric_forest",
                  "aerialhell:cortinarius_violaceus_forest",
                  "aerialhell:lapis_robinia_savana",
                  "aerialhell:copper_pine_forest",
                  "aerialhell:aerial_tree_forest",
                  "aerialhell:crystal_forest",
                  "aerialhell:aerial_hell_plains",
                  "aerialhell:crystal_plains"
                ]
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": {
                  "Name": "aerialhell:stellar_grass_block"
                }
              }
            },
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:biome",
                "biome_is": [
                  "aerialhell:shadow_forest",
                  "aerialhell:shadow_plain"
                ]
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": {
                  "Name": "aerialhell:shadow_grass_block"
                }
              }
            },
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:biome",
                "biome_is": [
                  "aerialhell:slippery_sand_ocean"
                ]
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": {
                  "Name": "aerialhell:slippery_sand"
                }
              }
            }
          ]
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:stone_depth",
          "offset": 0,
          "surface_type": "floor",
          "add_surface_depth": true,
          "secondary_depth_range": 0
        },
        "then_run": {
          "type": "minecraft:sequence",
          "sequence": [
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:not",
                "invert": {
                  "type": "minecraft:biome",
                  "biome_is": [
                    "aerialhell:slippery_sand_ocean"
                  ]
                }
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": {
                  "Name": "aerialhell:stellar_dirt"
                }
              }
            },
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:biome",
                "biome_is": [
                  "aerialhell:slippery_sand_ocean"
                ]
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": {
                  "Name": "aerialhell:slippery_sand"
                }
              }
            }
          ]
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:not",
          "invert": {
            "type": "minecraft:vertical_gradient",
            "random_name": "stellar_dirt_top",
            "true_at_and_below": {
              "absolute": 180
            },
            "false_at_and_above": {
              "absolute": 230
            }
          }
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "aerialhell:stellar_dirt"
          }
        }
      }
    ]
  }
}